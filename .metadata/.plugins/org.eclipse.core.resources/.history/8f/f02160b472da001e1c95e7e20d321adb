package recursividade;

import java.util.Scanner;

public class Fibo {

	public static void main(String[] args) {
		Scanner sc = new Scanner(Ssytem.in);
		int num = sc.nextInt();
	}

	public int fibo(int num) {
		if (num <= 1) {
			return num;
		} else {
			return fibo(num - 1) + fibo(num - 2);
		}
	}
}
