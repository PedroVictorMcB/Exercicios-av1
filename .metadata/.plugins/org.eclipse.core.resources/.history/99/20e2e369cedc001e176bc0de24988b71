package exerciciosSala;

public class AvengersList {

	private int size;
	private int[] list;
	private int i;
	
	public AvengersList(int size) {
		if (size > 0) {
			this.size = size;
		} else {
			this.size = 1;
		}
		
		list = new int[this.size];
		this.i = 0;
	}
	
	public void add(int gf) {
		if(i < size) {
			list[i] = gf;
			i++;
		}
	}
}
