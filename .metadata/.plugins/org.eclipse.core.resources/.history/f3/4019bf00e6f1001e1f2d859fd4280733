package Prova;

import java.util.Scanner;

public class Teste {

	public static void main(String[] args) {
		String[] txtList = {};
		arrStrMain(txtList, "Peter");
		
		

	}
	
	public void arrStr(String[] list, String obrigatorio ,String opt) {
	    String option = opt;
	    list[0] = obrigatorio;
	    list[1] = option;
	}

	public void arrStrMain(String[] list,String obrigatorio) {
	    arrStr(list, obrigatorio,"node");
	}


}
