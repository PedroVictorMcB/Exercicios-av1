package exerciciosSala;

import java.util.Vector;

public class AvengersList {

	private int limit;
	Vector<String> vect;
	
	public AvengersList(int limit) {
		if(limit > 0) {
			this.limit = limit;
		} else {
			this.limit = 1;
		}
		
		vect = new Vector<String>(this.limit);
	}
	
	public Vector<String> peterParkerGF(String nome){
		for (int i = 0; i < this.limit; i++) {
			vect.add(i, nome);
		}
		
		return vect; 
	}
	
	public int kraven() {
		vect.get(limit);
	}
}
