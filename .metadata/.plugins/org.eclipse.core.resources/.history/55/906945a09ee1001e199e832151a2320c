package tadList;

public class ListTreino {

	private int size;
	private Object[] list;
	private int i = 0;
	
	public static void main(String[] args) {
		

	}
	
	public ListTreino(int size) {
		if(size > 0) {
			this.size = size;
		} else {
			size = 1;
		}
		list = new Object[this.size];
	}

	public int sizeOf() {
		return size;
	}
}
